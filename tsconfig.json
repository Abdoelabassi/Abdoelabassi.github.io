{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "/src/types/global.d.ts",
    "/src/env.d.ts"
  ],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["@astrojs/image/client"],
    "strictNullChecks": true,
    "allowJs": true,
    "verbatimModuleSyntax": false
  }
}
